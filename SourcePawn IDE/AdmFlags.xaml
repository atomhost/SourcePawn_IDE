<Window x:Class="SourcePawn_IDE.AdmFlags"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Admin Flag Editor" Height="236" Width="286" ResizeMode="NoResize" WindowStyle="ToolWindow" ShowInTaskbar="False">
